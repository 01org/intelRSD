{
    "agent": {
        "vendor" : "Intel Corporation",
        "capabilities" : [ "Network" ]
    },
    "server": {
        "port": 7779
    },
    "registration": {
        "ipv4": "127.0.0.1",
        "port": 8383,
        "interval": 3
    },
    "managers": [
        {
            "ipv4": "127.0.0.1",
            "serialConsoleEnabled": true,
            "switches": [
                {
                    "locationOffset": 10,
                    "parentId": "1",
                    "mgmt_port": "ma1",
                    "ports" : [
                        { "id": "Ethernet33", "portType": "Upstream" }
                    ]
                }
            ]
        }
    ],
    "service-uuid-file" : "/var/opt/psme/network-service-uuid.json",
    "logger" : {
        "agent" : {
            "level" : "WARNING",
            "timeformat" : "DATE_NS",
            "color" : true,
            "output" : true,
            "tagging" : true,
            "moredebug" : false,
            "streams" : [
                {
                    "type" : "STDOUT"
                }
            ]
        }
    }
}
